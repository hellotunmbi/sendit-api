{
  "name": "sendit-api",
  "version": "1.0.0",
  "description": "NodeJS API for SendIT",
  "main": "server.js",
  "scripts": {
    "test": "mocha --require babel-polyfill --compilers js:babel-register",
    "start": "node dist/server.js",
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "dev": "nodemon server.js --exec babel-node --presets babel-preset-env"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hellotunmbi/sendit-api.git"
  },
  "keywords": [],
  "author": "Olutunmbi Banto",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hellotunmbi/sendit-api/issues"
  },
  "homepage": "https://github.com/hellotunmbi/sendit-api#readme",
  "dependencies": {
    "babel-preset-env": "^1.7.0",
    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.4.0",
    "moment": "^2.22.2",
    "morgan": "^1.9.1",
    "pg": "^7.6.1",
    "rimraf": "^2.6.2",
    "shortid": "^2.2.14",
    "swagger-ui-express": "^4.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.6",
    "rimraf": "^2.6.2"
  },
  "engines": {
    "node": "8.11.3",
    "npm": "6.4.1"
  }
}
